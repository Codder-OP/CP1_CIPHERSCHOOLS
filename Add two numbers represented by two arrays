#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    int ar1[n];
	    for(int i=0;i<n;i++){
	        cin>>ar1[i];
	    }
	    int m;
	    cin>>m;
	    int ar2[m];
	    for(int i=0;i<m;i++){
	        cin>>ar2[i];
	    }
	    int num1=0,num2=0,sum=0;
	    for(int i=0;i<n;i++)
	    {
	        num1=num1*10+ar1[i];
	    }
	    for(int i=0;i<m;i++)
	    {
	        num2=num2*10+ar2[i];
	    }
	    sum=num1+num2;
	    cout<<sum<<endl;
	}
	return 0;
}
