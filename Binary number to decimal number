#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    long long b;
	    cin>>b;
	    int ans=0,p=0;
	    
	    while(b>0)
	    {
	        int l=b%10;
	        ans+=l*pow(2,p);
	        b/=10;
	        p++;
	    }
	    cout<<ans<<endl;
	}
	return 0;
}
