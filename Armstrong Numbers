#include <iostream>
#include<bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int n;
	    cin>>n;
	    int temp=n;
	    int sum=0;
	    while(temp>0)
	    {
	        int l=temp%10;
	        sum+=pow(l,3);
	        temp/=10;
	    }
	    if(sum==n)
	        cout<<"Yes"<<endl;
	    else
	        cout<<"No"<<endl;
	}
	return 0;
}
